<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.tmobile.cabu.entities">
  <class name="ContractKey" table="CONTRACTKEY">
		<id name="key" type="java.lang.Integer" unsaved-value="null">
            <column name="KEY" sql-type="INTEGER"/>
            <generator class="native">
            </generator>
        </id>	  
	  
		<property name="sss" type="java.lang.String">
            <column name="NAME" not-null="true" unique="false" sql-type="VARCHAR(256)"/>
        </property>

	  </class>
</hibernate-mapping>