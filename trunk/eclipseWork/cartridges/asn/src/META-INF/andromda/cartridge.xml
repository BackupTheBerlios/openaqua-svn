<cartridge>

    <templateEngine className="org.andromda.templateengines.FreeMarker">
    </templateEngine>


    <!-- define the template objects that are made available to the template -->
    <templateObject name="stringUtils" className="org.apache.commons.lang.StringUtils"/>

    <!-- cartridge-property merge-point -->

    <template
        path="templates/asn/asnDescription.ftl"
        outputPattern="{0}/{1}.html"
        outlet="forms"
        overwrite="true"
        outputToSingleFile="true"
        outputOnEmptyElements="true">
        <modelElements variable="AsnDescription">
            <modelElement>
                <type name="de.openaqua.dev.cartridges.asn.metafacades.AsnFacade"/>
            </modelElement>
        </modelElements>
    </template>

</cartridge>