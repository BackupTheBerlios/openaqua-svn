<andromda>
    <properties>
        <property name="failOnValidationErrors">false</property>
        <property name="loggingConfigurationUri">file:${basedir}/conf/test/log4j.xml</property>
    </properties>
    <repositories>
         <repository name="emf-uml2">
            <models>
                <model type="emf-uml2">
                    <uri>${test.model.uri}</uri>
                    <modelPackages processAll="true" />
                </model>
            </models>
        </repository>
    </repositories>
    <namespaces>
        <namespace name="default">
            <properties>
                <property name="languageMappingsUri">Java</property>
                <property name="wrapperMappingsUri">JavaWrapper</property>
                <property name="jdbcMappingsUri">JDBC</property>
            </properties>
        </namespace>
        <namespace name="asn1">
            <properties>
                <property name="asnDescriptionFiles">${test.output.dir}</property>
            </properties>
        </namespace>
    </namespaces>
</andromda>
