<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  This is used to map the model types to
  hibernate types.
-->
<mappings name="NHibernate">
    <mapping>
        <from>datatype::Time</from>
        <from>datatype::Date</from>
        <from>datatype::DateTime</from>
        <from>datatype::Timestamp</from>
        <to>NHibernate.Nullables2.NullableDateTimeType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::char</from>
        <to>Char</to>
    </mapping>
    <mapping>
        <from>datatype::Character</from>
        <to>NHibernate.Nullables2.NullableCharType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::byte</from>
        <to>Byte</to>
    </mapping>
    <mapping>
        <from>datatype::Byte</from>
        <to>NHibernate.Nullables2.NullableByteType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::short</from>
        <to>Int16</to>
    </mapping>
    <mapping>
        <from>datatype::Short</from>
        <to>NHibernate.Nullables2.NullableInt16Type, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::int</from>
        <to>Int32</to>
    </mapping>
    <mapping>
        <from>datatype::Integer</from>
        <to>NHibernate.Nullables2.NullableInt32Type, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::long</from>
        <to>Int64</to>
    </mapping>
    <mapping>
        <from>datatype::Long</from>
        <to>NHibernate.Nullables2.NullableInt64Type, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::float</from>
        <to>float</to>
    </mapping>
    <mapping>
        <from>datatype::Float</from>
        <to>NHibernate.Nullables2.NullableSingleType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::double</from>
        <to>Double</to>
    </mapping>
    <mapping>
        <from>datatype::Double</from>
        <from>datatype::Money</from>
        <to>NHibernate.Nullables2.NullableDoubleType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::Decimal</from>
        <to>NHibernate.Nullables2.NullableDecimalType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::Boolean</from>
        <to>NHibernate.Nullables2.NullableBooleanType, NHibernate.Nullables2</to>
    </mapping>
    <mapping>
        <from>datatype::boolean</from>
        <to>Boolean</to>
    </mapping>
    <mapping>
        <from>datatype::Guid</from>
        <to>Guid</to>
    </mapping>
    <mapping>
        <from>datatype::Clob</from>
        <to>StringClob</to>
    </mapping>
    <mapping>
        <from>datatype::Blob</from>
        <to>BinaryBlob</to>
    </mapping>
</mappings>