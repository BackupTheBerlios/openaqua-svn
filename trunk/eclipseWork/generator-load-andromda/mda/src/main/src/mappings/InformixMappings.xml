<?xml version="1.0" encoding="ISO-8859-1" ?>
<!--
  This is used to map the model types to
  Sybase types for generated code.
  Information was determined from:
  http://www-306.ibm.com/software/data/informix/pubs/library/datablade/dbdk/sqlt/09.fm1.html#117108
-->
<mappings name="Informix">
    <mapping>
        <from>datatype::String</from>
        <to>VARCHAR(255)</to>
    </mapping>
    <mapping>
        <from>datatype::Date</from>
        <to>DATE</to>
    </mapping>
    <mapping>
        <from>datatype::Time</from>
        <to>DATETIME</to>
    </mapping>
    <mapping>
        <from>datatype::Timestamp</from>
        <from>datatype::DateTime</from>
        <to>DATETIME</to>
    </mapping>
    <mapping>
        <from>datatype::char</from>
        <from>datatype::Character</from>
        <to>CHAR</to>
    </mapping>
    <mapping>
        <from>datatype::byte</from>
        <from>datatype::Byte</from>
        <to>SMALLINT</to>
    </mapping>
    <mapping>
        <from>datatype::short</from>
        <from>datatype::Short</from>
        <to>SMALLINT</to>
    </mapping>
    <mapping>
        <from>datatype::int</from>
        <from>datatype::Integer</from>
        <to>INTEGER</to>
    </mapping>
    <mapping>
        <from>datatype::long</from>
        <from>datatype::Long</from>
        <to>DECIMAL</to>
    </mapping>
    <mapping>
        <from>datatype::float</from>
        <from>datatype::Float</from>
        <to>FLOAT</to>
    </mapping>
    <mapping>
        <from>datatype::double</from>
        <from>datatype::Double</from>
        <from>datatype::Money</from>
        <to>DECIMAL</to>
    </mapping>
    <mapping>
        <from>datatype::boolean</from>
        <from>datatype::Boolean</from>
        <to>BOOLEAN</to>
    </mapping>
    <mapping>
        <from>datatype::Decimal</from>
        <from>datatype::decimal</from>
        <to>DECIMAL</to>
    </mapping>
    <mapping>
        <from>datatype::Clob</from>
        <to>CLOB</to>
    </mapping>
    <mapping>
        <from>datatype::Blob</from>
        <to>BLOB</to>
    </mapping>
</mappings>